{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:staged": "vue-cli-service serve --mode staging",
    "serve:production": "vue-cli-service serve --mode production",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint --fix",
    "clean": "rm -rf dist",
    "clean:production": "npm run clean && aws s3 rm s3://app-dashboard --recursive",
    "clean:staged": "npm run clean && aws s3 rm s3://app-dashboard-staged --recursive",
    "deploy:production": "npm install && npm run clean:production && AWS_S3_DEPLOY=production vue-cli-service build",
    "deploy:staged": "npm install && npm run clean:staged && AWS_S3_DEPLOY=staged vue-cli-service build --mode staging"
  },
  "dependencies": {
    "@casl/ability": "^4.1.6",
    "@vuex-orm/core": "^0.36.3",
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "d3": "^5.15.1",
    "debounce-async": "0.0.2",
    "dotenv": "^8.2.0",
    "file-saver": "^2.0.2",
    "localforage": "^1.7.3",
    "lodash": "^4.17.15",
    "moment": "^2.29.1",
    "reflect-metadata": "^0.1.13",
    "register-service-worker": "^1.7.1",
    "soundcloud": "^3.3.2",
    "url-parse": "^1.4.7",
    "vue": "^2.6.12",
    "vue-property-decorator": "^8.4.2",
    "vue-router": "^3.4.7",
    "vue-the-mask": "^0.11.1",
    "vuetify": "^2.3.15",
    "vuex": "^3.5.1",
    "vuex-orm-decorators": "git+https://github.com/hamishbrindle/vuex-orm-decorators.git#b9c5124dcf8d7583e7932bb893378c3935e20dca",
    "vuex-pathify": "^1.4.1",
    "vuex-persist": "^2.3.0"
  },
  "devDependencies": {
    "@types/d3": "^5.16.3",
    "@types/file-saver": "^2.0.1",
    "@types/jest": "^24.0.19",
    "@types/lodash": "^4.14.162",
    "@types/node": "^13.13.28",
    "@types/url-parse": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "@vue/cli-plugin-babel": "~4.5.8",
    "@vue/cli-plugin-e2e-cypress": "~4.5.8",
    "@vue/cli-plugin-eslint": "~4.5.8",
    "@vue/cli-plugin-pwa": "^4.5.8",
    "@vue/cli-plugin-router": "~4.5.8",
    "@vue/cli-plugin-typescript": "~4.5.8",
    "@vue/cli-plugin-unit-jest": "~4.5.8",
    "@vue/cli-plugin-vuex": "~4.5.8",
    "@vue/cli-service": "~4.5.8",
    "@vue/eslint-config-airbnb": "^5.1.0",
    "@vue/eslint-config-typescript": "^5.1.0",
    "@vue/test-utils": "1.0.0-beta.31",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-vue": "^6.2.2",
